{
  "manifest_version": 2,
  "name": "Lunaxy",
  "version": "1.0",
  "permissions": ["proxy", "privacy"],
  "background": {
    "scripts": ["popup.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}
