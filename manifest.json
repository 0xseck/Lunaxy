{
  "manifest_version": 2,
  "browser_specific_settings":{
    "gecko":{
        "id": "lunaxy@lunaxy.proxy",
        "strict_min_version":"91.1.0",
        "update_url":"https://seck.fun/Lunaxy/updates.json"
    }
  },
  "name": "Lunaxy",
  "version": "0.2.0",
  "permissions": ["proxy", "privacy", "<all_urls>", "storage"],
  "background": {
    "scripts": ["popup.js"],
    "persistent": false
  },
  "browser_action":   {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}
